<h2>Log in</h2>

<%=  form_tag sessions_path do %>
  <p>
    <%=  label_tag :email %>
    <%=  text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag "Log in", class: "btn btn-primary" %>
  </p>
<% end %>